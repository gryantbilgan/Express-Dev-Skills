<%- include('../partials/header') %>

<form action="/skills/<%= skill.id%>?_method=PUT" method="POST">
    <input type="text" name="skill" value="<%= skill.skill %>"><br>
    <label>
        <input type="checkbox" name="learned" <%= skill.learned ? 'checked' : '' %>>
        Comprehend
    </label><br>
    <button type="submit">Level Up</button>
</form>

<%- include('../partials/footer') %>